{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "google", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {},
  "overrides": [
    {
      "files": ["./**"],
      "rules": {
        "require-jsdoc": "off"
      }
    }
  ]
}
